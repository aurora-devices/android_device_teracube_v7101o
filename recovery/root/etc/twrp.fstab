# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK
# 
# 
# <mount_point>   <fstype>  <device>              <device2>                     <flags>

# Core Partitions
/metadata         emmc      /dev/block/platform/bootdevice/by-name/metadata     flags=backup=1;display="Metadata"
/data             ext4      /dev/block/platform/bootdevice/by-name/userdata     flags=encryptable=/dev/block/platform/bootdevice/by-name/metadata
/cache            ext4      /dev/block/platform/bootdevice/by-name/cache        flags=display="Cache"
/boot             emmc      /dev/block/platform/bootdevice/by-name/boot         flags=backup=1;display="Boot";flashimg=1
/dtbo             emmc      /dev/block/platform/bootdevice/by-name/dtbo         flags=backup=1;display="Dtbo";flashimg=1
/recovery         emmc      /dev/block/platform/bootdevice/by-name/recovery     flags=display="Recovery"
/persist          ext4      /dev/block/platform/bootdevice/by-name/persist      flags=backup=1;display="Persist"
/persist_image    emmc      /dev/block/platform/bootdevice/by-name/persist      flags=backup=1;display="Persist Image";flashimg=1
/system           ext4      /dev/block/platform/bootdevice/by-name/system       flags=backup=0;display="System"
/system_image     emmc      /dev/block/platform/bootdevice/by-name/system       flags=backup=1;display="System Image";flashimg=1
/vendor           ext4      /dev/block/platform/bootdevice/by-name/vendor       flags=backup=0;display="Vendor"wipeingui
/vendor_image     emmc      /dev/block/platform/bootdevice/by-name/vendor       flags=backup=1;display="Vendor Image";flashimg=1

# MediaTek partition
/bootloader       emmc      /dev/block/platform/bootdevice/by-name/lk           flags=backup=1;display="Bootloader 1";flashimg=1
/bootloader2      emmc      /dev/block/platform/bootdevice/by-name/lk2          flags=backup=1;display="Bootloader 2";flashimg=1
/logo             emmc      /dev/block/platform/bootdevice/by-name/logo         flags=backup=1;display="Logo";flashimg=1
/nvcfg            ext4      /dev/block/platform/bootdevice/by-name/nvcfg        flags=backup=1;display="Nvcfg";flashimg=1
/nvdata           ext4      /dev/block/platform/bootdevice/by-name/nvdata       flags=backup=1;display="Nvdata";flashimg=1
/nvram            emmc      /dev/block/platform/bootdevice/by-name/nvram        flags=backup=1;display="Nvram";flashimg=1
/protect_f        ext4      /dev/block/platform/bootdevice/by-name/protect1     flags=backup=1;display="Protect F";flashimg=1
/protect_s        ext4      /dev/block/platform/bootdevice/by-name/protect2     flags=backup=1;display="Protect S";flashimg=1
/tee1             emmc      /dev/block/platform/bootdevice/by-name/tee1         flags=backup=1;display="Trustzone 1";flashimg=1
/tee2             emmc      /dev/block/platform/bootdevice/by-name/tee2         flags=backup=1;display="Trustzone 2";flashimg=1

# External storage
/external_sd      auto      /dev/block/mmcblk1p1  /dev/block/mmcblk1            flags=display="External SD";storage;wipeingui;removable;fsflags="utf8"
/usbotg           auto      /dev/block/sda1       /dev/block/sda                flags=display="USB OTG";storage;wipeingui;removable;fsflags="utf8"
